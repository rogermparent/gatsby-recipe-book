{
  "name": "gatsby-recipe-book",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby-recipe-book",
  "author": "rogermparent",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "clean": "npm -w editor -w website run clean",
    "develop": "npm -w editor run develop",
    "editor": "npm -w editor run develop",
    "editor-public": "npm -w editor run -- develop -H 0.0.0.0",

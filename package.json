    "date-fns": "^2.30.0",
    "fraction.js": "^4.2.0",
    "fs-extra": "^11.1.1",
    "gatsby": "^5.10.0",
    "gatsby-plugin-image": "^3.10.0",
    "gatsby-plugin-manifest": "^5.10.0",
    "gatsby-plugin-sharp": "^5.10.0",
    "gatsby-plugin-sitemap": "^6.10.0",
    "gatsby-plugin-vanilla-extract": "^4.0.1",

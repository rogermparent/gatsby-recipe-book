    "develop-website": "npm -w website run develop",
    "serve": "npm -w website run serve",
    "prepare": "husky install",
    "lint": "eslint './**/*.{js,ts,jsx,tsx}'"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
